{
  "name": "hn-client",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.15.1+sha512.34e538c329b5553014ca8e8f4535997f96180a1d0f614339357449935350d924e22f8614682191264ec33d1462ac21561aff97f6bb18065351c162c7e8f6de67",
  "scripts": {
    "dev": "concurrently \"pnpm mobile\" \"pnpm web\" --names \"mobile,web\" --prefix-colors \"cyan,magenta\"",
    "mobile": "pnpm --filter @hn/mobile start",
    "mobile:ios": "pnpm --filter @hn/mobile ios",
    "mobile:ios:widgets": "pnpm --filter @hn/mobile ios:widgets",
    "mobile:prebuild": "pnpm --filter @hn/mobile prebuild:ios",
    "mobile:prebuild:clean": "pnpm --filter @hn/mobile prebuild:clean",
    "mobile:android": "pnpm --filter @hn/mobile android",
    "mobile:web": "pnpm --filter @hn/mobile web",
    "mobile:lint": "pnpm --filter @hn/mobile lint",
    "web": "pnpm --filter @hn/web dev",
    "web:build": "pnpm --filter @hn/web build",
    "web:start": "pnpm --filter @hn/web start",
    "web:lint": "pnpm --filter @hn/web lint",
    "shared:typecheck": "pnpm --filter @hn/shared typecheck",
    "typecheck": "pnpm -r typecheck",
    "lint": "pnpm -r lint",
    "clean": "pnpm -r exec rm -rf node_modules .expo dist .next",
    "reset": "pnpm clean && rm -rf node_modules pnpm-lock.yaml && pnpm install"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "typescript": "~5.9.2"
  }
}
